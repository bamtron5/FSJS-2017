{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "client/*.js",
    "server/*.es6",
    "server/*.jsx"
  ],
  "dontLoad": [
    "node_modules/**",
    "bower_components/**",
    "client/bundle.js"
  ],
  "plugins": {
    "es_modules": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "node_resolve": {},
    "angular": {},
    "commonjs": {}
  }
}
