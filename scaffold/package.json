{
  "name": "scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "server/bin/www",
  "scripts": {
    "lint": "jshint",
    "build:sass": "node-sass -r ./client/styles/ -o ./client/styles/",
    "build:server": "babel server -x .es6 --out-dir server",
    "build:client": "webpack",
    "build": "npm run build:server && npm run build:client",
    "watch:sass": "node-sass -r -w ./client/styles/ -o ./client/styles/",
    "watch:server": "babel server -x .es6 --out-dir server -w",
    "watch:client": "webpack --watch",
    "dev": "concurrently \"nodemon\" \"npm run watch:sass\" \"npm run watch:server\" \"npm run watch:client\"",
    "start": "node server/bin/www"
  },
